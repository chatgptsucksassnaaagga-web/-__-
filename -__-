local HttpService = game:GetService("HttpService")
local TeleportService = game:GetService("TeleportService")
local Players = game:GetService("Players")
local player = Players.LocalPlayer
local PLACE_ID = game.PlaceId
local RELAY_URL = "https://YOUR_RELAY_URL/servers"

local template = G2L["7"]
template.Visible = false

local function clearLogs()
	for _,v in ipairs(G2L["6"]:GetChildren()) do
		if v:IsA("Frame") and v ~= template then
			v:Destroy()
		end
	end
end

local function addLog(data,y)
	local f = template:Clone()
	f.Visible = true
	f.Position = UDim2.new(0.03175,0,0,y)
	f.Parent = G2L["6"]
	f["Brainrot name"].Text = data.brainrot or "Unknown"
	f.MPS.Text = tostring(data.mps)
	f["Join button"].MouseButton1Click:Connect(function()
		TeleportService:TeleportToPlaceInstance(PLACE_ID,data.jobId,player)
	end)
end

local function refresh()
	clearLogs()
	local ok,res = pcall(function()
		return HttpService:JSONDecode(game:HttpGet(RELAY_URL))
	end)
	if not ok then return end
	table.sort(res,function(a,b) return (a.mps or 0) > (b.mps or 0) end)
	local y = 0.02406
	for _,s in ipairs(res) do
		if s.placeId == PLACE_ID then
			addLog(s,y)
			y += 0.17
		end
	end
end

G2L["f"].MouseButton1Click:Connect(refresh)
G2L["12"].MouseButton1Click:Connect(clearLogs)
G2L["16"].MouseButton1Click:Connect(refresh)

refresh()

return G2L["1"]
